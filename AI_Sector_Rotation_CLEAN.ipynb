      "source": [
        "# 36.0 - Calculate Relative Performance\n",
        "df_sector_pct_change = df_sector.pct_change().dropna()\n",
        "rolling_mean_pct_change = df_sector_pct_change.rolling(window=12).mean().dropna()\n",
