      "source": [
        "# 23.0 - LSTM Model\n",
        "lstm_model = Sequential([\n",
        "    LSTM(64, activation=\"relu\", return_sequences=True, input_shape=(seq_length, X.shape[2])),\n",
        "    LSTM(64, activation=\"relu\"),\n",
        "    Dropout(0.01),\n",
