        "\n",
        "for sector in sector_etfs:\n",
        "    relative_performance[sector] = rolling_mean_pct_change[sector] - rolling_mean_pct_change[\"S&P 500\"]\n",
        "\n",
        "print(\"Relative performance calculated successfully!\")\n",
        "display(relative_performance.head())"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# 37.0 - Clean Relative Performance\n",
        "rolling_mean_pct_change_cleaned = rolling_mean_pct_change.dropna()\n",
